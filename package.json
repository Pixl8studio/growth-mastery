{
  "name": "growth-mastery",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "pnpm exec prettier --write \"**/*.{ts,tsx,md,json}\"",
    "format:check": "pnpm exec prettier --check \"**/*.{ts,tsx,md,json}\"",
    "test": "node scripts/validate-test-safety.js && vitest run",
    "test:unit": "vitest run __tests__/unit",
    "test:unit:fast": "vitest run __tests__/unit --reporter=dot",
    "test:integration": "vitest run __tests__/integration",
    "test:watch": "node scripts/validate-test-safety.js && vitest --watch",
    "test:coverage": "node scripts/validate-test-safety.js && vitest run --coverage",
    "test:ci": "node scripts/validate-test-safety.js && vitest run --coverage --reporter=verbose",
    "test:ui": "node scripts/validate-test-safety.js && vitest --ui",
    "test:validate": "node scripts/validate-test-safety.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "db:types": "supabase gen types typescript --local > types/database.ts || echo '⚠️  Skipping type generation (no local Supabase running)'",
    "db:types:remote": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > types/database.ts",
    "db:validate": "tsx scripts/validate-supabase.ts",
    "db:migrations-check": "tsx scripts/check-migrations.ts",
    "db:test-connection": "tsx scripts/test-supabase-connection.ts",
    "db:test-auth": "tsx scripts/test-auth-signup.ts",
    "db:check-config": "tsx scripts/check-supabase-config.ts",
    "db:format": "test -n \"$(ls supabase/migrations/*.sql 2>/dev/null)\" && sql-formatter --language postgresql --output supabase/migrations/*.sql || echo '⚠️  No migration files to format'",
    "db:format-check": "test -n \"$(ls supabase/migrations/*.sql 2>/dev/null)\" && (for file in supabase/migrations/*.sql; do sql-formatter --language postgresql \"$file\" | diff -q \"$file\" - || exit 1; done) || echo '⚠️  No migration files to check'",
    "pre-push": "pnpm exec run-s pre-push:checks pre-push:test",
    "pre-push:checks": "run-p lint format:check type-check db:migrations-check test:enforce-location",
    "pre-push:test": "pnpm test:unit:fast",
    "pre-push:full": "pnpm exec run-s pre-push:checks test:ci",
    "test:enforce-location": "node scripts/enforce-test-location.js",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:all": "rm -rf .next out node_modules",
    "prepare": "husky",
    "claude": "claude",
    "claude:review": "claude review",
    "claude:chat": "claude chat",
    "swarm": "tsx scripts/swarm/orchestrator.ts",
    "swarm:dry-run": "tsx scripts/swarm/orchestrator.ts --dry-run",
    "swarm:verbose": "tsx scripts/swarm/orchestrator.ts --verbose",
    "swarm:config": "scripts/swarm/generate-agents-config.sh",
    "swarm:health": "scripts/swarm/swarm-health.sh",
    "swarm:deploy": "scripts/swarm/deploy-agents.sh",
    "swarm:tunnel": "scripts/swarm/tunnel-agents.sh start",
    "swarm:tunnel:stop": "scripts/swarm/tunnel-agents.sh stop",
    "swarm:tunnel:status": "scripts/swarm/tunnel-agents.sh status"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.76.1",
    "@upstash/ratelimit": "^2.0.4",
    "@vapi-ai/server-sdk": "^0.10.2",
    "@vapi-ai/web": "^2.5.0",
    "@vercel/kv": "^3.0.0",
    "cheerio": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.26",
    "jszip": "^3.10.1",
    "lucide-react": "^0.546.0",
    "mammoth": "^1.8.0",
    "next": "16.0.7",
    "openai": "^6.6.0",
    "pdfjs-dist": "^4.8.69",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-markdown": "^10.1.0",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "typescript": "^5.9.3",
    "xlsx": "^0.18.5",
    "yaml": "^2.8.2",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^2.0.25",
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.9.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-config-next": "16.0.7",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.5",
    "node-sql-parser": "^5.3.13",
    "npm-run-all2": "^8.0.4",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "sql-formatter": "^15.6.10",
    "supabase": "^2.53.6",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vercel": "^48.9.1",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "22.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.18.0",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "supabase/migrations/*.sql": [
      "sql-formatter --language postgresql --fix"
    ]
  }
}
